{
	"compilerOptions": {
		"module": "ES6",
		"target": "ES5",
		"moduleResolution": "node",
		"allowJs": true,
		"checkJs": true,
		"resolveJsonModule": true,
		"traceResolution": true,
		"declaration": true,
		"baseUrl": ".",
		"typeRoots": [
			"./@types/modules"
		]
	},
	"include": [
		"src/**/*.ts",
	],
}